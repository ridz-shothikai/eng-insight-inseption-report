# Example environment variables for docker-compose
# Copy to .env and edit values before running `docker compose up`.

# GitHub Container Registry image settings
OWNER=ridz-shothikai
REPO=eng-insight-inseption-report
# IMAGE_TAG should be the 7-char commit SHA tag (e.g. a1b2c3d)
IMAGE_TAG=latest

# App runtime configuration
UPLOAD_DIR=./uploads
OUTPUT_DIR=./output

# Ports
HOST_PORT=8080
CONTAINER_PORT=8080

# Optional API keys
GOOGLE_API_KEY=
GOOGLE_CSE_ID=
GOOGLE_API_KEY= api-GOOGLE_API_KEY
GOOGLE_CSE_ID= api-GOOGLE_CSE_ID
IMAGE_API_KEY= api-IMAGE_API_KEY

ENABLE_TOOLS= true

OUTPUT_PATH= output/
UPLOAD_PATH= uploads/

## create database
MONGODB_URL=your-mongodb-url
MONGODB_DB_NAME=your-database-name